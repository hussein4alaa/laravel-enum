{
    "name": "g4t/enum",
    "description": "PHP Enums",
    "license": "MIT",
    "authors": [
        {
            "name": "HusseinAlaa",
            "email": "hussein4alaa@gmail.com",
            "homepage": "https://github.com/hussein4alaa"
        }
    ],
    "homepage": "https://github.com/g4t/enum",
    "keywords": ["Laravel", "Enum"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "g4t\\Enum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "g4t\\Enum\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "g4t\\Enum\\EnumServiceProvider"
            ],
            "aliases": {
                "Enum": "g4t\\Enum\\Facades\\Enum"
            }
        }
    }
}
